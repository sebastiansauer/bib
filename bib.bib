@article{offenbacherSpiritualityInternationalClassification2011,
  title = {Spirituality and the {{International Classification}} of {{Functioning}}, {{Disability}} and {{Health}}: Content Comparison of Questionnaires Measuring Mindfulness Based on the {{International Classification}} of {{Functioning}}},
  author = {Offenbächer, Martin and Sauer, S and Hieblinger, Robin and Hufford, David J and Walach, Harald and Kohls, Niko and Offenbcher, M and Sauer, S and Hieblinger, Robin and Hufford, David J and Walach, Harald and Kohls, Niko and Offenbächer, Martin and Sauer, S and Hieblinger, Robin and Hufford, David J and Walach, Harald and Kohls, Niko and Offenbaecher, Martin},
