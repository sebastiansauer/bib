@article{wieczorekPsychometricPropertiesMindfulness2022,
  title = {Psychometric {{Properties}} of the {{Mindfulness Inventory}} for {{Sport}} ({{German Version}})},
  author = {Wieczorek, Alissa and Renner, Karl-Heinz and Schrank, Florian and Seiler, Kirstin and Wagner, Matthias},
  date = {2022-05-24},
  journaltitle = {Frontiers in Psychology},
  shortjournal = {Front. Psychol.},
  volume = {13},
  pages = {864208},
  issn = {1664-1078},
  doi = {10.3389/fpsyg.2022.864208},
  url = {https://www.frontiersin.org/articles/10.3389/fpsyg.2022.864208/full},
  urldate = {2023-06-23},
  abstract = {Mindfulness-based training programs are highly established in competitive and recreational sports. One of the best-known approaches is the Mindfulness-Acceptance-Commitment Approach (MAC) by Gardner and Moore), which integrates mindfulness aspects of awareness, non-judgmental attitude, and focus. Based on these aspects, Thienot and colleagues developed and validated an English language sport-specific questionnaire, the so-called Mindfulness Inventory for Sport (MIS), for the assessment of mindfulness skills in athletes. The aim of this study is to psychometrically test a German language version of the MIS (MIS-D). To assess the psychometric properties, the MIS-D was examined in an online survey with an integrated test–retest design (               n               \,=\,228) for reliability (internal consistency; test–retest reliability), validity (factorial; convergent), and measurement invariance (gender; competition type). The present results support the psychometric quality of the German language version of the MIS. Necessary replications should among others focus on checking the measurement invariance for further relevant subgroups.}
}

