  title = {Blunted Ventral Striatal Responses to Anticipated Rewards Foreshadow Problematic Drug Use in Novelty-Seeking Adolescents.},
  author = {Büchel, Christian and Peters, Jan and Banaschewski, Tobias and Bokde, Arun L W and Bromberg, Uli and Conrod, Patricia J and Flor, Herta and Papadopoulos, Dimitri and Garavan, Hugh and Gowland, Penny and Heinz, Andreas and Walter, Henrik and Ittermann, Bernd and Mann, Karl and Martinot, Jean-Luc and Paillère-Martinot, Marie-Laure and Nees, Frauke and Paus, Tomas and Pausova, Zdenka and Poustka, Luise and Rietschel, Marcella and Robbins, Trevor W and Smolka, Michael N and Gallinat, Juergen and Schumann, Gunter and Knutson, Brian and IMAGEN consortium, Mercedes and Artiges, Eric and Aydin, Semiha and Bach, Christine and Barbot, Alexis and Barker, Gareth and Bruehl, Ruediger and Cattrell, Anna and Constant, Patrick and Crombag, Hans and Czech, Katharina and Dalley, Jeffrey and Decideur, Benjamin and Desrivieres, Sylvane and Fadai, Tahmine and Fauth-Buhler, Mira and Feng, Jianfeng and Filippi, Irinia and Frouin, Vincent and Fuchs, Birgit and Gemmeke, Isabel and Genauck, Alexander and Hanratty, Eanna and Heinrichs, Bert and Heym, Nadja and Hubner, Thomas and Ihlenfeld, Albrecht and Ing, Alex and Ireland, James and Jia, Tianye and Jones, Jennifer and Jurk, Sarah and Kaviani, Mehri and Klaassen, Arno and Kruschwitz, Johann and Lalanne, Christophe and Lanzerath, Dirk and Lathrop, Mark and Lawrence, Claire and Lemaitre, Hervé and Macare, Christine and Mallik, Catherine and Mar, Adam and Martinez-Medina, Lourdes and Mennigen, Eva and family=Carvahlo, given=Fabiana Mesquita, prefix=de, useprefix=true and Mignon, Xavier and Millenet, Sabina and Miranda, Ruben and Müller, Kathrin and Nymberg, Charlotte and Parchetka, Caroline and Pena-Oliver, Yolanda and Pentilla, Jani and Poline, Jean-Baptiste and Quinlan, Erin Burke and Rapp, Michael and Ripke, Stephan and Ripley, Tamzin and Robert, Gabriel and Rogers, John and Romanowski, Alexander and Ruggeri, Barbara and Schmäl, Christine and Schmidt, Dirk and Schneider, Sophia and Schubert, Florian and Schwartz, Yannick and Sommer, Wolfgang and Spanagel, Rainer and Speiser, Claudia and Spranger, Tade and Stedman, Alicia and Stephens, Dai and Strache, Nicole and Ströhle, Andreas and Struve, Maren and Subramaniam, Naresh and Theobald, David and Vetter, Nora and Vulser, Helene and Weiss, Katharina and Whelan, Robert and Williams, Steve and Xu, Bing and Yacubian, Juliana and Yu, Tao and Ziesch, Veronika},
  date = {2017-02},
  journaltitle = {Nature communications},
  volume = {8},
  eprint = {28221370},
  eprinttype = {pmid},
  pages = {14140},
  issn = {2041-1723},
  doi = {10.1038/ncomms14140},
  url = {http://www.nature.com/doifinder/10.1038/ncomms14140 http://www.ncbi.nlm.nih.gov/pubmed/28221370 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5321762},
  abstract = {Novelty-seeking tendencies in adolescents may promote innovation as well as problematic impulsive behaviour, including drug abuse. Previous research has not clarified whether neural hyper- or hypo-responsiveness to anticipated rewards promotes vulnerability in these individuals. Here we use a longitudinal design to track 144 novelty-seeking adolescents at age 14 and 16 to determine whether neural activity in response to anticipated rewards predicts problematic drug use. We find that diminished BOLD activity in mesolimbic (ventral striatal and midbrain) and prefrontal cortical (dorsolateral prefrontal cortex) regions during reward anticipation at age 14 predicts problematic drug use at age 16. Lower psychometric conscientiousness and steeper discounting of future rewards at age 14 also predicts problematic drug use at age 16, but the neural responses independently predict more variance than psychometric measures. Together, these findings suggest that diminished neural responses to anticipated rewards in novelty-seeking adolescents may increase vulnerability to future problematic drug use.},
  bdsk-url-1 = {http://www.nature.com/doifinder/10.1038/ncomms14140\%20http://www.ncbi.nlm.nih.gov/pubmed/28221370\%20http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5321762},
  bdsk-url-2 = {https://dx.doi.org/10.1038/ncomms14140}
}

