  date = {2015-09-30},
  url = {https://www.semanticscholar.org/paper/Mindfulness-and-the-mindfulness-assessment-scales-Silpakit-Suthithammanuwat/22a87d7b2b3c4c6fbb211f3088c397e599d3a11d},
