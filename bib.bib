@inproceedings{mandlSocialPerceptionRobots2022,
  title = {The {{Social Perception}} of {{Robots Scale}} ({{SPRS}}): {{Developing}} and {{Testing}} a {{Scale}} for {{Successful Interaction Between Humans}} and {{Robots}}},
