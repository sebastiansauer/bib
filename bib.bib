@article{Black2011,
  title = {Psychometric {{Assessment}} of the {{Mindful Attention Awareness Scale}} ({{MAAS}}) {{Among Chinese Adolescents}}},
  author = {Black, David S and Sussman, Steve and Johnson, C Anderson and Milam, Joel},
  date = {2011-03},
  journaltitle = {Assessment},
