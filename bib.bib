@article{Goossen1998,
  title = {A {{Comparison}} of {{Nursing Minimal Data Sets}}},
  author = {Goossen, William T F and Epping, Paul J M M and Feuth, Ton and Dassen, Theo W N and Hasman, Arie and family=Heuvel, given=Wim J A, prefix=van den, useprefix=true},
