  volume = {35},
  number = {1},
  pages = {45--58},
  issn = {0044-3514},
  doi = {10.1024/0044-3514.35.1.45},
  url = {wiese@uni-landau.de%5Cn10.1024/0044-3514.35.1.45%5Cnhttp://ezproxy.lib.utexas.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2004-14886-005&site=ehost-live},
  abstract = {Work and family play a central but potentially conflicting role in adults' lives. We conducted a cross-sectional as well as a two-week-diary study with 35 dual-earner couples (N = 70 working adults, 51.3 \% married). Besides work-family conflict, we measured variables such as subjective involvement in the work and family domains, emotional well-being, and personal goals. Work-family conflicts were partly interdependent between the two partners: everyday conflicts due to negative influences of the partnership domain were positively interrelated. Work-family conflict were mainly predicted by psychological variables (e. g., subjective involvement in the professional domain), whereas sociodemographic parameters (e. g., gender) were of minor importance. Cross-sectionally as well as processually work-family conflicts were related to negative emotions. Positive emotions were mainly influenced by individual goal progress and by perceiving the partner as content and supportive. (PsycINFO Database Record (c) 2007 APA, all rights reserved) (from the journal abstract)},
  bdsk-url-1 = {wiese@uni-landau.de\%5Cn10.1024/0044-3514.35.1.45\%5Cnhttp://ezproxy.lib.utexas.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true\&db=psyh\&AN=2004-14886-005\&site=ehost-live},
  bdsk-url-2 = {https://dx.doi.org/10.1024/0044-3514.35.1.45},
  isbn = {0044-3514},
  keywords = {Couples,Dual Careers,dual earner couples,Emotional States,emotional well-being,emotions,family involvement,Family Work Relationship,personal goals,Role Conflicts,social support,subjective involvement,Well Being,work involvement,work-family conflict}
}

