@article{Kuijpers2007,
  title = {Meditation-Induced Psychosis.},
  author = {Kuijpers, H J H and family=Heijden, given=F M M, prefix=a, useprefix=true and Tuinier, S and family=Verhoeven, given=W M, prefix=a, useprefix=false},
