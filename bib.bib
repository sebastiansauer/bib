@article{Matney2008,
  title = {Translation and Integration of {{CCC}} Nursing Diagnoses into {{ICNP}}},
  author = {Matney, Susan A and DaDamio, Rebecca and Couderc, Carmela and Dlugos, Mary and Evans, Jonathan and Gianonne, Gay and Haskell, Robert and Hardiker, Nicholas and Coenen, Amy and Saba, Virginia K},
  date = {2008},
  journaltitle = {Journal of the American Medical Informatics Association: JAMIA},
  volume = {15},
  number = {6},
  pages = {791--793},
  abstract = {The purpose of this study was to translate and integrate nursing diagnosis concepts from the Clinical Care Classification (CCC) System Version 2.0 to DiagnosticPhenomenon or nursing diagnostic statements in the International Classification for Nursing Practice (ICNP) Version 1.0. Source concepts for CCC were mapped by the project team, where possible, to pre-coordinated ICNP terms. The manual decomposition of source concepts according to the ICNP 7-Axis Model served to validate the mappings. A total of 62\% of the CCC Nursing Diagnoses were a pre-coordinated match to an ICNP concept, 35\% were a post-coordinated match and only 3\% had no match. During the mapping process, missing CCC concepts were submitted to the ICNP Programme, with a recommendation for inclusion in future releases.},
  keywords = {Controlled,Documentation International Council of Nurses Nurs}
}

@article{Matousek2010c,
  title = {Cortisol as a Marker for Improvement in Mindfulness-Based Stress Reduction},
  author = {Matousek, Rose H. and Dobkin, Patricia L. and Pruessner, Jens},
  date = {2010-02},
  journaltitle = {Complementary Therapies in Clinical Practice},
  volume = {16},
  number = {1},
  eprint = {20129404},
  eprinttype = {pmid},
  pages = {13--19},
  publisher = {{Elsevier Ltd}},
  issn = {17443881},
  doi = {10.1016/j.ctcp.2009.06.004},
  url = {http://www.ncbi.nlm.nih.gov/pubmed/20129404 http://dx.doi.org/10.1016/j.ctcp.2009.06.004},
  abstract = {While much attention has been devoted to examining the beneficial effects of Mindfulness-Based Stress Reduction programs on patients' ability to cope with various chronic medical conditions, most studies have relied on self-report measures of improvement. Given that these measures may not accurately reflect physiological conditions, there is a need for an objective marker of improvement in research evaluating the beneficial effects of stress management programs. Cortisol is the major stress hormone in the human organism and as such is a promising candidate measure in the study of the effects of Mindfulness-Based Stress Reduction programs. In conjunction with other biological measures, the use of cortisol levels as a physiological marker of stress may be useful to validate self-reported benefits attributed to this program. In the current manuscript, we review the available literature on the role of cortisol as a physiological marker for improvement with regards to mindfulness practice, and make recommendations for future study designs. ?? 2009 Elsevier Ltd. All rights reserved.},
  bdsk-url-1 = {http://www.ncbi.nlm.nih.gov/pubmed/20129404\%20http://dx.doi.org/10.1016/j.ctcp.2009.06.004},
  bdsk-url-2 = {https://dx.doi.org/10.1016/j.ctcp.2009.06.004},
  isbn = {1873-6947 (Electronic)-3881 (Linking)},
  keywords = {Adaptation,Biological Markers,Biological Markers: blood,Cortisol,Female,Humans,Hydrocortisone,Hydrocortisone: blood,Male,MBSR,Meditation,Metaphysical,Mind-Body Relations,Mindfulness-Based Stress Reduction,Mindfulness-Based Stress Reduction Cortisol Stress,Psychological,Psychological: blood,Psychological: physiology,Psychological: therapy,Relaxation Therapy,Relaxation Therapy: methods,Stress}
}

@article{Matousek2011,
  title = {Changes in the Cortisol Awakening Response ({{CAR}}) Following Participation in {{Mindfulness-Based Stress Reduction}} in Women Who Completed Treatment for Breast Cancer},
  author = {Matousek, Rose H and Pruessner, Jens C and Dobkin, Patricia L},
  date = {2011},
  journaltitle = {Complementary Therapies in Clinical Practice},
  volume = {17},
  number = {2},
  pages = {65--70},
  abstract = {Background Changes in the cortisol awakening response (CAR) were studied in women participating in a Mindfulness-Based Stress Reduction (MBSR) program after completion of their medical treatment for breast cancer.Method Thirty-three women completed questionnaires pre- and post-MBSR pertaining to: stress, depressive symptomatology, and medical symptoms. The CAR was assessed on 3 days pre- and 3 days post-MBSR as a biological marker of stress.Results A significant effect on the CAR was found, with cortisol levels showing a prolonged increase after awakening at the post-MBSR assessment period. This was accompanied by significant improvements in self-reported stress levels, depressive symptomatology, and medical symptoms. Furthermore, the change in medical symptoms was negatively correlated with the area under the curve (AUC) at study onset (r = -.52, p {$<$} .002); i.e., the greater the AUC of the CAR before MBSR, the greater the reduction in medical symptoms after the program.Conclusions These results suggest the potential usefulness of employing the CAR as a biological marker in women with breast cancer participating in an MBSR program.},
  keywords = {Breast cancer Cortisol Mindfulness-based stress re}
}

@article{Matrices2012,
  title = {Advanced {{Progressive Matrices}}},
  author = {Matrices, Advanced Progressive},
  date = {2012},
  pages = {71--76},
  abstract = {Diagnostische Zielsetzung:Bei den Advanced Progressive Matrices (APM) handelt es sich um ein haeufig eingesetztes und bewaehrtes Verfahren zur Messung des sprachunabhaengigen logischen Denkens. Die APM gehoeren zur Verfahrensgruppe der Raven-Matrizen-Tests (RPM) und stellen in diesem Rahmen eine Version fuer ein ueberdurchschnittliches kognitives Leistungsniveau dar. Durch den Einsatz eines Zeitlimits kann die intellektuelle Effizienz der Probanden beurteilt werden.Aufbau:Die APM-Items bestehen aus geometrischen Figuren bzw. Mustern, in denen je ein Element ausgelassen wurde. Aus acht Antwortalternativen ist diejenige zu waehlen, die das Muster korrekt ergaenzt. Die Aufgaben sind nach Schwierigkeitsgrad geordnet und in zwei separaten Aufgabenheften zu den Aufgabensets 1 und 2 zusammengefasst. Das erste Set umfasst 12 Aufgaben (APM I), das zweite 36 Aufgaben (APM II). APM I wird entweder zur Vorbereitung des Einsatzes von APM II oder als Screeninginstrument eingesetzt. APM II ist lediglich bei Personen mit hinreichend hohem intellektuellen Leistungsniveau einzusetzen, um unnoetige Frustrationen zu vermeiden. Die gewaehlte Antwortalternative ist auf einem Durchschreibebogen bzw. in der computerisierten Version auf dem Monitor anzukreuzen.Grundlagen und Konstruktion:Die APM wurden 1943 zur Personalauslese im militaerischen Bereich eingesetzt und 1947 fuer den allgemeinen Gebrauch veroeffentlicht. Basierend auf Analysen der Aufgabenschwierigkeit wurden die Items nach ansteigender Schwierigkeit geordnet. Eine erste Revision der APM und eine Reduktion der urspruenglich 48 Items auf 36 Items (APM II) erfolgten 1962. In den aktuellen Auflagen des Verfahrens wurden die Items und ihre Reihenfolge nicht mehr modifiziert. Die mit den RPM gemessene Faehigkeit, Beziehungen zu stiften, wird von Raven als Faehigkeit beschrieben, aus vorgegebenen Informationen ein neues Verstaendnis bzw. neue Informationen abzuleiten.Empirische Prüfung und Gütekriterien:Aktuelle Manuale fuer die APM liegen von Raven et al. (1998b) sowie Heller et al. (1998) vor. Beide Manuale zitieren zahlreiche Studien, die mit den APM und den RPM als Verfahrensgruppe durchgefuehrt worden sind.Reliabilität: Die Reliabilitaetskoeffizienten fuer APM I fallen deutlich niedriger aus als fuer APM II. Als Masse der internen Konsistenz ermittelten Raven et al. (1998b, S. 24) fuer die APM II Split-half-Reliabilitaetskoeffizienten zwischen .83 und .87, fuer APM I Werte um .73. Heller et al. (1998, S. 12) ermittelten Alphawerte zwischen .34 und .78 (APM I) sowie zwischen .70 und .88 (APM II; Schueler der Klassen 6 bis 12). Nach einem Intervall von 3 Monaten ermittelten Heller et al. (1998, S. 13) an Schuelerstichproben Retestreliabilitaetskoeffizienten zwischen .58 und .82.Validität: Verwiesen wird auf signifikante Korrelationen zwischen APM und anderen Verfahren zur Messung intellektueller Faehigkeiten. Die Korrelationen mit sprachfreien Verfahren (z.B. Wechsler-Handlungs-IQ: r = .55) liegen ueber denen mit sprachbezogenen Verfahren (z.B. Wechsler-Verbal-IQ: r = .42; 131 Schueler). Korrelationen zwischen APM und den quantitativen und nonverbalen Skalen des Kognitiven Faehigkeitstests (.26 bis .44) bestaetigen insbesondere die Komponente des analogen Denkens in den APM (98 hochbegabte Schueler der 9./10. Klasse Gymnasium; 59 Schueler der 10. Klasse Regelgymnasium). Mit der beim Zahlenverbindungstest zentralen Komponente der Informationsverarbeitungsgeschwindigkeit weisen die APM zwar signifikante, aber nicht sehr hohe Zusammenhaenge von .34 (APM I) bzw. .33 (APM II) auf (114 Schueler der 8./9. Klasse). Mit den Mathematikschulnoten der 8. bzw. 9. Klasse (223 Schueler) bzw. den Deutschnoten wiesen die APM Korrelationen in Hoehe von .34 (APM I) und .35(APM II) bzw. .23 (APM I) und .27 (APM II) auf. Hinweise auf eine Geschlechtsabhaengigkeit der APM-Ergebnisse liegen nicht vor.Normen: Die deutschen Referenzwerte (Prozentraenge) von Raven et al. (1998b) basieren auf einer Stichprobe von 1142 Personen (48.5\% weiblich, 51,5\% maennlich; 13-75 Jahre). Ein vorhandenes Zeitlimit sowie unterschiedliche Bildungsniveaus und Berufsgruppen werden beruecksichtigt. Die Normen der APM-Ausgabe von Heller et al. (1998) beruhen auf einer deutschsprachigen Stichprobe von 581 Studierenden, 921 Schuelern und 157 Senioren ueber 60 Jahren. Ferner werden Daten von Normierungsstichproben aus Australien, China, Deutschland, Grossbritannien, Neuseeland, Nordirland, Polen sowie den USA aufgefuehrt.}
}

