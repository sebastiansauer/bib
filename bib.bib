@article{Englich2006,
  title = {Playing Dice with Criminal Sentences: The Influence of Irrelevant Anchors on Experts' Judicial Decision Making.},
  author = {Englich, Birte and Mussweiler, Thomas and Strack, Fritz},
  date = {2006-02},
  journaltitle = {Personality \& social psychology bulletin},
