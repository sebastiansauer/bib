@article{Tang2009,
  title = {Central and Autonomic Nervous System Interaction Is Altered by Short-Term Meditation},
  author = {Tang, Yi-Yuan Y and Ma, Yinghua and Fan, Yaxin and Feng, Hongbo and Wang, Junhong and Feng, Shigang and Lu, Qilin and Hu, Bing and Lin, Yao and Li, Jian and Zhang, Ye and Wang, Yan and Zhou, Li and Fan, Ming},
