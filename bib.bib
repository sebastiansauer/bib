  keywords = {Adult,Analysis of Variance,Child,Child Abuse,{Child Abuse, Sexual},Cross-Sectional Studies,Female,Humans,Inpatients,Life Change Events,Male,Risk Factors,Sex Factors,Substance-Related Disorders,Surveys and Questionnaires}
