@article{Lepilliez2008a,
  title = {Endoscopic Resection of Sporadic Duodenal Adenomas : An Efficient Technique with a Substantial Risk of Delayed Bleeding},
  author = {Lépilliez, V and Chemaly, M and Ponchon, T and Napoleon, B and Saurin, J C},
