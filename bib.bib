@article{Cardaciotto2008c,
  title = {The Assessment of Present-Moment Awareness and Acceptance: {{The Philadelphia Mindfulness Scale}}.},
  author = {Cardaciotto, LeeAnn A and Herbert, James D and Forman, Evan M and Moitra, Ethan and Farrow, Victoria},
  date = {2008-07},
