@article{Hell2008,
  title = {Synopse Der {{Hohenheimer Metaanalysen}} Zur {{Prognostizierbarkeit}} Des {{Studienerfolgs}} Und {{Implikationen}} Für Die {{Auswahl-}} Und {{Beratungspraxis}}},
  author = {Hell, Benedikt and Trapmann, Sabrina and Schuler, Heinz},
  date = {2008},
  journaltitle = {Studierendenauswahl und Studienentscheidung},
  pages = {43--54},
  abstract = {Die zentralen Ergebnisse von drei aktuellen Metaanalysen zur Prognostizierbarkeit des Studienerfolgs werden vorgestellt, und die für die Auswahl- und Bewerbungspraxis relevanten Schlussfolgerungen werden deutlich gemacht. Die drei Metaanalysen integrieren Studien, die international (Prädiktor unstrukturiertes oder strukturiertes Interview), in Europa (Prädiktor Schulnoten) oder im deutschsprachigen Raum (Prädiktor Studierfähigkeitstests) seit 1980 publiziert worden sind (44, 53 und 36 Studien). Die Metaanalysen machen unter anderem deutlich, dass Schulnoten den besten Einzelprädiktor von Studienerfolg im Sinne von Schulnoten darstellen, dass fachspezifische Studierfähigkeitstests eine etwas geringere Validität haben und dass die prognostische Validität von Auswahlgesprächen für Studiennoten vergleichsweise gering ist. Angesichts der Komplexität des Konstrukts der Studieneignung und der häufig heterogenen Ziele, die mit Auswahl- und Beratungssystemen verfolgt werden, und auch aufgrund der psychometrischen Überlegenheit wird ein multimethodales Vorgehen für empfehlenswert gehalten.},
  isbn = {978-3-8017-2103-9},
  keywords = {*Academic Achievement Prediction,*Colleges,*Higher Education,*Hochschulausbildung,*Hochschulen,*Leistungsprognose (Schule und Hochschule),*Student Admission Criteria,*Zulassungsbestimmungen (Schule und Hochschule),Academic Learning \& Achievement,Aptitude Measures,College Academic Achievement,Educational Diagnosis,Educational Measurement,Eignungstests,Grading (Educational),Interviews,Lernen und Leistung LA - German,Measurement,Messung,Notengebung,Pädagogische Diagnose,Pädagogische Messung und Beurteilung,prediction of college students' academic achieveme,Studienleistung (Hochschule)}
