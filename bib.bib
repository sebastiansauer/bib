@online{lehmannHowHeavyTailed2023,
  type = {SSRN Scholarly Paper},
  title = {How {{Do Heavy Tailed Distributions Affect}} the {{Analysis}} of {{Psychometric Bipolar Scales Data}}?},
  author = {Lehmann, René and Vogt, Bodo},
  date = {2023-04-12},
  number = {4412962},
  location = {{Rochester, NY}},
  doi = {10.2139/ssrn.4412962},
  url = {https://papers.ssrn.com/abstract=4412962},
  urldate = {2023-07-20},
  abstract = {Psychometric data of bipolar scales are commonly used in medical and economic psychology. Recently, their compositional structure (the Simplex) was revealed. Disregarding the Simplex causes serious bias, e.g., while estimating correlations or psychometric standards. The isometric log-ratio (ilr) transformation converts Simplex data towards the interval scale and provides unbiased statistical results. The ilr approach increases the statistical power of the well-known correlation test based on Student’s t-distribution if the prerequisites of the central limit theorem (CLT) are fulfilled. Concerning ilr transformed data, the CLT is applicable if and only if the number of summands is sufficiently large and the data generating process (DGP) provides finite variance. In practice, however, the number of items of a questionnaire often is small and the DGP could be heavy-tailed. By means of a simulation study, this paper shows that the ilr approach works satisfactory if the DGP is heavy-tailed with finite (Laplace) or infinite (Cauchy) variance. A violation of the CLT hardly affects the results of the ilr approach. The results of the simulation study generalize from correlation analysis towards a larger set of standard statistical procedures, e.g. linear regression, two-sample t-test and partial least squares structural equation modeling.},
  langid = {english},
  pubstate = {preprint},
  keywords = {bipolar scale,compositional data,correlation,heavy-tailed distribution,psychometrics,Social Psychology,statistical power}
}

