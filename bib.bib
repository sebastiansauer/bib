@article{Tong2006,
  title = {Neural Bases of Binocular Rivalry.},
  author = {Tong, Frank and Meng, Ming and Blake, Randolph},
  date = {2006-11},
  journaltitle = {Trends in cognitive sciences},
  volume = {10},
