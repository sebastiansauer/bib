@article{Caprara2012,
  title = {The {{Positivity Scale}}.},
  author = {Caprara, Gian Vittorio and Alessandri, Guido and Eisenberg, Nancy and Kupfer, a. and Steca, Patrizia and Caprara, Maria Giovanna and Yamaguchi, Susumu and Fukuzawa, Ai and Abela, John},
  date = {2012},
