@article{Pirolli1985,
  title = {The Role of Learning from Examples in the Acquisition of Recursive Programming Skills},
  author = {Pirolli, Peter L. and Anderson, John R.},
  date = {1985},
  journaltitle = {Canadian Journal of Psychology},
  volume = {39},
