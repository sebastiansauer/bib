@article{Urrila2017,
  title = {Sleep Habits, Academic Performance, and the Adolescent Brain Structure.},
  author = {Urrila, Anna S and Artiges, Eric and Massicotte, Jessica and Miranda, Ruben and Vulser, Hélène and Bézivin-Frere, Pauline and Lapidaire, Winok and Lema\^itre, Hervé and Penttilä, Jani and Conrod, Patricia J and Garavan, Hugh and Paillère Martinot, Marie-Laure and Martinot, Jean-Luc and IMAGEN consortium, Tobias and Flor, Herta and Fauth-Bühler, Mira and Poutska, Louise and Nees, Frauke and Grimmer, Yvonne and Struve, Maren and Heinz, Andeas and Ströhle, Andreas and Kappel, Viola and family=Noort, given=Betteke Maria, prefix=van, useprefix=true and Poline, Jean-Baptiste and Schwartz, Yanick and Thyreau, Benjamin and Ireland, James and Rogers, John and Bordas, Nadège and Bricaud, Zuleima and Filippi, Irina and Galinowski, André and Gollier-Briant, Fanny and Ménard, Vincent and Schumann, Gunter and Desrivières, Sylvane and Cattrell, Anna and Goodman, Robert and Stringaris, Argyris and Nymberg, Charlotte and Reed, Laurence and Barker, Gareth J and Ittermann, Berndt and Brühl, Ruediger and Smolka, Michael and Hübner, Thomas and Müller, Kathrin and Bokde, Arun L W and Büchel, Christian and Bromberg, Uli and Gallinat, Jurgen and Fadai, Tahmine and Gowland, Pennylaire and Lawrence, C and Paus, Tomas},
